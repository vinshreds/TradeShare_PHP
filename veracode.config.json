{
    "sca": {
        "output_directory": "./veracode-sca-output",
        "scan_timeout": 3600,
        "debug": true,
        "project_name": "TradeShare",
        "scan_type": "php",
        "include_patterns": [
            "*.php",
            "*.js",
            "*.css",
            "*.html"
        ],
        "exclude_patterns": [
            "vendor/*",
            "node_modules/*",
            "uploads/*",
            "images/*"
        ]
    }
} 